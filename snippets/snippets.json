{
	"import styled from 'styled-components'": {
		"prefix": "imsc",
		"body": "import styled from 'styled-components';",
		"description": "import styled from 'styled-components'"
	},
	"import styled from 'styled-components/native'": {
		"prefix": "imscn",
		"body": "import styled from 'styled-components/native';",
		"description": "import styled from 'styled-components/native'"
	},
	"import { injectGlobal } from 'styled-components'": {
		"prefix": "imscg",
		"body": "import { injectGlobal } from 'styled-components';",
		"description": "import { injectGlobal } from 'styled-components'"
	},
	"import { withTheme } from 'styled-components'": {
		"prefix": "imsct",
		"body": "import { withTheme } from 'styled-components';",
		"description": "import { withTheme } from 'styled-components'"
	},
	"Styled-Component": {
		"prefix": "sc",
		"body": "const ${1} = styled.${2}`\n  ${3}\n`;",
		"description": "Styled-Component"
	},
	"Export styled-component": {
		"prefix": "exsc",
		"body": "export const ${1} = styled.${2}`\n  ${3}\n`;",
		"description": "Export styled-component"
	},
	"Styled-Component from existing component": {
		"prefix": "scc",
		"body": "const Styled${1} = styled(${1})`\n  ${2}\n`;",
		"description": "Styled-Component from existing component"
	},
	"Export styled-component from existing component": {
		"prefix": "exscc",
		"body": "export const Styled${1} = styled(${1})`\n  ${2}\n`;",
		"description": "Export styled-component from existing component"
	},
	"Styled-Components file": {
		"prefix": "scf",
		"body":
			"import styled from 'styled-components';\n\nconst ${1} = styled.${2}`\n  ${3}\n`;\n\nexport default ${1};",
		"description": "Styled-Components file"
	},
	"innerRef (shorthand)": {
		"prefix": "inr",
		"body": "innerRef={x => this.${1} = x}",
		"description": "innerRef (shorthand)"
	},
	"innerRef": {
		"prefix": "innerRef",
		"body": "innerRef={x => this.${1} = x}",
		"description": "innerRef"
	},
	"styled-component extends": {
		"prefix": "scext",
		"body": "const Extended${1} = ${1}.extends`\n  ${2}\n`;",
		"description": "styled-component extends"
	},
	"Export styled-component extends": {
		"prefix": "exscext",
		"body": "export const Extended${1} = ${1}.extends`\n  ${2}\n`;",
		"description": "Export styled-component extends"
	},
	"ThemeProvider": {
		"prefix": "ThemeProvider",
		"body": "<ThemeProvider theme={${1}}>\n  ${2}\n</ThemeProvider>",
		"description": "ThemeProvider"
	},
	"styled-component theme": {
		"prefix": "sct",
		"body":
			"// Define what props.theme will look like\nconst theme = {\n  ${1}: '${2}'\n};",
		"description": "styled-component theme"
	},
	"injectGlobal": {
		"prefix": "injectGlobal",
		"body": "injectGlobal`\n  ${1}\n`;",
		"description": "injectGlobal"
	},
	"Styled-Component with attributes": {
		"prefix": "scattrs",
		"body": "const ${1} = styled.${2}.attrs({\n  ${3}: ${4}\n})`\n  ${5}\n`;",
		"description": "Styled-Component with attributes"
	},
	"Export styled-component with attributes": {
		"prefix": "exscattrs",
		"body":
			"export const ${1} = styled.${2}.attrs({\n  ${3}: ${4}\n})`\n  ${5}\n`;",
		"description": "Export styled-component with attributes"
	}
}
